{%- assign base_font = settings.regular__font -%}
{%- assign base_font_bolder = base_font | font_modify: 'weight', 'bolder' -%}
{%- assign logo_font = settings.logo__font -%}
{%- assign logo_font_bolder = logo_font | font_modify: 'weight', 'bolder' -%}
<link rel="preload" href="{{ base_font | font_url }}" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ base_font_bolder | font_url }}" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="{{ logo_font_bolder | font_url }}" as="font" type="font/woff2" crossorigin>
<style>
    {{ base_font | font_face: font_display: 'swap' }}
    {{ base_font_bolder | font_face: font_display: 'swap' }}
    {{ logo_font_bolder | font_face: font_display: 'swap' }}
    @font-face {
      font-family: "flex-icon";
      src: url(data:application/font-woff;base64,) format("woff");
      font-weight: normal;
      font-style: normal;
      font-display: swap;
    }
</style>